<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-deploy</actionName>
            <displayName>deploy</displayName>
            <goals>
                <goal>deploy</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-Clean and Build (snapshots)</actionName>
            <displayName>Clean and Build (snapshots)</displayName>
            <goals>
                <goal>clean</goal>
                <goal>install</goal>
            </goals>
        </action>
        <action>
            <actionName>run</actionName>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.1.1:exec</goal>
            </goals>
            <properties>
                <exec.classpathScope>runtime</exec.classpathScope>
                <exec.args>-classpath %classpath de.cismet.lagis.gui.main.LagisApp</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
    </actions>
